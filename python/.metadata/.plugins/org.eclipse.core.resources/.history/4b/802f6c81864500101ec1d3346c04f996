def factorial(number):
    if number==1:
        return 1
    return number*factorial(number-1)
def sum_toE(n):
    retv=2
    for x in range(2,n):
        retv += 1/factorial(x) 
    return retv   
        

        
if __name__ == '__main__':
    retv = factorial(4)
    print retv