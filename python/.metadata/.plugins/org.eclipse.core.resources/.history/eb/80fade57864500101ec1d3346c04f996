def factorial(number):
    if number==1:
        return 1
    return number*factorial(number-1)
def sum_toE(count):
    retv=0
    for x in range(1,count):
        retv += 1/factorial(x) 
        
        

        
if __name__ == '__main__':
    retv = factorial(4)
    print retv